				<type>enum-of-string</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<value-definitions>
					<value-definition>
						<display xml:lang="x-default">Static Embed</display>
						<value>staticEmbed</value>
					</value-definition>
					<value-definition>
						<display xml:lang="x-default">Dynamic Embed</display>
						<value>dynamicEmbed</value>
					</value-definition>
				</value-definitions>
			</attribute-definition>
			<attribute-definition attribute-id="turntoSetupTypeRR">
				<display-name xml:lang="x-default">Setup Type Ratings &amp; Reviews</display-name>
				<description xml:lang="x-default">Type of presentation used for TurnTo Reviews functionality. Static Embed is recommended for SEO benefits.</description>
				<type>enum-of-string</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<value-definitions>
					<value-definition>
						<display xml:lang="x-default">Static Embed</display>
						<value>staticEmbed</value>
					</value-definition>
					<value-definition>
						<display xml:lang="x-default">Dynamic Embed</display>
						<value>dynamicEmbed</value>
					</value-definition>
				</value-definitions>
