				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>www.turnto.com</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="turntoQAEnable">
				<display-name xml:lang="x-default">Q&amp;A</display-name>
				<description xml:lang="x-default">Enable the TurnTo Question &amp; Answers feature</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>true</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="turntoReviewsEnable">
				<display-name xml:lang="x-default">Reviews</display-name>
				<description xml:lang="x-default">Enable the TurnTo Reviews feature</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>true</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="turntoCategoryReviewTeaserEnable">
				<display-name xml:lang="x-default">Category Review Teaser</display-name>
				<description xml:lang="x-default">Enable Review Teaser on the Category and Search Pages</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
			</attribute-definition>
