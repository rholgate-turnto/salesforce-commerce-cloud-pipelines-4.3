				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>true</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="turntoCommentsPinboardEnable">
				<display-name xml:lang="x-default">Checkout Comments Pinboard</display-name>
