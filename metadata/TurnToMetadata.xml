				<display-name xml:lang="x-default">Category Comment Teaser</display-name>
				<description xml:lang="x-default">Enable the checkout comments teaser below the product name on product category pages
				</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
			</attribute-definition>
			<attribute-definition attribute-id="turntoReviewsEnable">
				<display-name xml:lang="x-default">Reviews</display-name>
				<description xml:lang="x-default">Enable Reviews in Teaser and Widget on Product Detail Page</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
			</attribute-definition>
			<attribute-definition attribute-id="turntoVisualGalleryEnable">
				<display-name xml:lang="x-default">Visual Content Gallery Row Widget</display-name>
				<description xml:lang="x-default">Enables the PDP Gallery Row Widget</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
				<default-value>true</default-value>
			</attribute-definition>
			<attribute-definition attribute-id="turntoVisualPinboardEnable">
				<display-name xml:lang="x-default">Visual Content Pinboard</display-name>
				<description xml:lang="x-default">Enables the Visual Content Pinboard</description>
				<type>boolean</type>
				<mandatory-flag>false</mandatory-flag>
				<externally-managed-flag>false</externally-managed-flag>
			</attribute-definition>
